{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "exclude": ["node_modules", "dist", "graphql/**/*"],
  "compilerOptions": {
    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,

    // "types": ["@nuxt/types", "@nuxt/schema", "@types/node"],
    "skipLibCheck": true,
  },
}
